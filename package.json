{
  "name": "data-input",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "package": "npm run build && pkg ."
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "csv-parse": "^5.0.0",
    "inquirer": "^8.2.5",
    "xlsx": "^0.18.0"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.5",
    "@types/node": "^16.11.7",
    "@types/xlsx": "^0.0.36",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.4.4"
  },
  "bin": "dist/index.js",
  "pkg": {
    "targets": [
      "node16-win-x64"
    ],
    "outputPath": "executable"
  }
}
